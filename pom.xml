	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<version>3.1.0</version>
				<configuration>
					<suiteXmlFiles>
						<suiteXmlFile>XMLFiles/membershipSuite.xml</suiteXmlFile>
						Â 
					</suiteXmlFiles>
					<!-- <systemPropertyVariables> <url>${url}</url> <browser>${browser}</browser> 
						<userame>${username}</userame> <password>${password}</password> </systemPropertyVariables> -->

					<properties>
						<property>
							<name>listener</name>
							<value>utility.MyTestListenerAdapter,utility.MyRetryAnalizer</value>
						</property>
					</properties>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
